   "source": [
    "first_set = pd.read_csv('Chicago_Crimes_2012_to_2017.csv')\n",
    "# Drop rows that have 2017 as the year.\n",
    "crimes = pd.DataFrame(first_set[first_set['Year'] != 2017])\n",
    "crimes.head()"
   ]
  },
